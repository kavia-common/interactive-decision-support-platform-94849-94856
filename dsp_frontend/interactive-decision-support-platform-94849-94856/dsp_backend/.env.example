# Backend environment variables (copy to .env for local/dev)
# IMPORTANT: Do NOT commit real secrets to source control.

# Metadata (optional)
PROJECT_NAME=DSP Backend API
PROJECT_DESCRIPTION=FastAPI backend for Decision Support Platform providing auth and agent endpoints.
PROJECT_VERSION=0.1.0

# Auth / Security
SECRET_KEY=please_change_me_to_a_strong_value
ACCESS_TOKEN_EXPIRE_MINUTES=60

# CORS
# Comma-separated list of allowed origins
CORS_ORIGINS=http://localhost:3000

# Database (default SQLite inside container or local run)
DATABASE_URL=sqlite:///./app.db
